{
    "extends": "eslint:recommended",
    "env": {
        "node": true
    },
    "rules": {
        "comma-dangle": [2, "never"],
        "curly": 0,
        "radix": 2,
        "wrap-iife": 2,
        "brace-style": 0,
        "comma-style": 2,
        "consistent-this": 0,
        "indent": [2, 4],
        "no-console": 0,
        "no-lonely-if": 2,
        "no-nested-ternary": 2,
        "no-use-before-define": [2, "nofunc"],
        "quotes": [2, "single"],
//        "space-before-function-paren": [2, "never"],
//        "space-after-keywords": [2, "always"],
//        "space-before-blocks": [2, "always"],
//        "space-in-parens": [2, "never"],
        "space-unary-ops": 2,
    }
}